{
  "name": "a6s-railway",
  "version": "0.1.10",
  "description": "K8s deployment orchestration tool.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "rimraf": "rimraf",
    "start": "node dist/cli.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "build": "rimraf ./dist/ && tsc",
    "prepublishOnly": "npm run build",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' 'test/**/*.ts'"
  },
  "bin": {
    "rw": "dist/cli.js"
  },
  "author": "API Geeks LLC",
  "license": "MIT",
  "dependencies": {
    "commander": "2.15.1",
    "deepmerge": "2.1.1",
    "ejs": "2.6.1",
    "flat": "4.0.0",
    "joi": "13.4.0",
    "js-yaml": "3.12.0",
    "mkdirp": "0.5.1",
    "tmp-promise": "1.0.4"
  },
  "devDependencies": {
    "@types/chai": "4.0.10",
    "@types/deepmerge": "2.1.0",
    "@types/ejs": "2.6.0",
    "@types/flat": "0.0.28",
    "@types/joi": "13.3.0",
    "@types/js-yaml": "3.11.1",
    "@types/mkdirp": "0.5.2",
    "@types/node": "10.3.1",
    "chai": "4.1.2",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.12",
    "mochawesome": "3.0.2",
    "nyc": "11.8.0",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.6",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "dist/lib/**/index.js",
      "dist/test/**/*.js"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    },
    {
      "name": "Igor Ognichenko",
      "email": "igor.ognichenko@fireblink.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apigeeks-com/a6s-railway.git"
  }
}
